# -- Elastic Search -- #
export OTOPS_PROFILE_ELASTIC_SEARCH_DOCKER_IMAGE_NAME=opensearchproject/opensearch
export OTOPS_PROFILE_ELASTIC_SEARCH_DOCKER_IMAGE_TAG=latest
