# -- Web App -- #
export OTOPS_PROFILE_WEBAPP_RELEASE=v0.8.2
export OTOPS_PROFILE_NGINX_DOCKER_IMAGE_NAME=macbre/nginx-http3
export OTOPS_PROFILE_NGINX_DOCKER_IMAGE_TAG=latest
